package edu.lev.ymf.slam4.bo;

/**
 * @author ymf
 *
 */
public class Adresse {
	private int id = -1;
	private int num;
	private String rue;
	private String ville;
	private String cp;
	
	/**
	 * @param num
	 * @param rue
	 * @param ville
	 * @param cp
	 */
	public Adresse(int num, String rue, String ville, String cp) {
		super();
		this.num = num;
		this.rue = rue;
		this.ville = ville;
		this.cp = cp;
	}

	/**
	 * @param id
	 * @param num
	 * @param rue
	 * @param ville
	 * @param cp
	 */
	public Adresse(int id, int num, String rue, String ville, String cp) {
		super();
		this.id = id;
		this.num = num;
		this.rue = rue;
		this.ville = ville;
		this.cp = cp;
	}
	/**
	 * @return the idAdd
	 */
	public int getId() {
		return id;
	}
	/**
	 * @param idAdd the idAdd to set
	 */
	public void setId(int idAdd) {
		this.id = idAdd;
	}
	/**
	 * @return the num
	 */
	public int getNum() {
		return num;
	}
	/**
	 * @param num the num to set
	 */
	public void setNum(int num) {
		this.num = num;
	}
	/**
	 * @return the rue
	 */
	public String getRue() {
		return rue;
	}
	/**
	 * @param rue the rue to set
	 */
	public void setRue(String rue) {
		this.rue = rue;
	}
	/**
	 * @return the ville
	 */
	public String getVille() {
		return ville;
	}
	/**
	 * @param ville the ville to set
	 */
	public void setVille(String ville) {
		this.ville = ville;
	}
	/**
	 * @return the cp
	 */
	public String getCp() {
		return cp;
	}
	/**
	 * @param cp the cp to set
	 */
	public void setCp(String cp) {
		this.cp = cp;
	}
	public String toString() {
		return "([" + this.id + "] - " + this.num + " - " + this.rue + " - " + this.cp + " - " + this.ville + ")";
	}
}
